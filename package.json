  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:push": "drizzle-kit push",
    "migrate:generate": "drizzle-kit generate",
    "migrate:up": "tsx scripts/migrate.ts",
    "migrate:phase5": "tsx scripts/run-phase5-migrations.ts",
    "migrate:phase6": "tsx scripts/run-phase6-migrations.ts",
    "db:indexes": "tsx scripts/apply-indexes.ts",
    "db:health": "tsx scripts/check-database-health.ts",
    "test": "npm run check-env && vitest run && playwright test",
    "test:unit": "npm run check-env && vitest run tests/unit",
    "test:integration": "npm run check-env && vitest run tests/integration",
    "test:e2e": "npm run check-env && playwright test",
    "test:e2e:headless": "npm run check-env && playwright test --headed=false",
    "test:e2e:headed": "npm run check-env && playwright test --headed",
    "test:e2e:debug": "playwright test --headed --debug",
    "test:auth": "npm run check-env && vitest run tests/**/*auth* && playwright test tests/e2e/*auth*",
    "test:cart": "npm run check-env && vitest run tests/**/*cart* && playwright test tests/e2e/*cart*",
    "test:checkout": "npm run check-env && vitest run tests/**/*checkout* && playwright test tests/e2e/*checkout*",
    "test:products": "npm run check-env && vitest run tests/**/*product* && playwright test tests/e2e/*product*",
    "test:wishlist": "npm run check-env && vitest run tests/**/*wishlist* && playwright test tests/e2e/*wishlist*",
    "test:orders": "npm run check-env && vitest run tests/**/*order*",
    "test:admin": "npm run check-env && vitest run tests/**/*admin* && playwright test tests/e2e/*admin*",
    "test:coverage": "vitest run --coverage",
    "test:backend": "vitest run tests/integration",
    "test:watch": "vitest watch",
    "test:ci": "npm run check-env && vitest run --coverage && playwright test --reporter=junit",
    "check-env": "tsx scripts/check-test-environment.ts",
    "test:setup": "tsx scripts/setup-test-database.ts",
    "test:seed": "tsx scripts/setup-test-database.ts"
  },
